{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "^/$", "dest": "/index.html" },
    { "src": "/api/(.*)", "dest": "/api/index.js" },
    { "src": "/(user|book)(/.*)?", "dest": "/api/index.js" },
    { "src": "/health(/.*)?", "dest": "/api/index.js" },
    { "src": "/test", "dest": "/api/index.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
 